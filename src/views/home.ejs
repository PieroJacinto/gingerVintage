<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('./partials/head') %>
    <!-- 1 owl carousel min.css -->
    <link 
        rel="stylesheet" 
        href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css" 
        integrity="sha512-tS3S5qG0BlhnQROyJXvNjeEM4UpMXHrQfTGmbQ1gKmelCxlSEBUaxhRBj/EFTzpbP4RVSrpEikbmdJobCvhE3g==" 
        crossorigin="anonymous" 
        referrerpolicy="no-referrer" 
    />
    <!-- 2. owl carousel theme Min.css-->
    <link 
        rel="stylesheet" 
        href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.theme.default.min.css" 
        integrity="sha512-sMXtMNL1zRzolHYKEujM2AqCLUR9F2C4/05cdbxjjLSRvMQIciEPCQZo++nk7go3BtSuK9kfa/s+a4f4i5pLkw==" 
        crossorigin="anonymous" 
        referrerpolicy="no-referrer" 
    /> 
</head>

<body>
    <!-- HEADER -->
    <header>
        <%- include('./partials/header') %>
    </header>   

    <!-- MAIN -->
    <main>
        <!-- BANNER  y CATEGORIAS-->
        <section class="container-fluid txt-ctr mt-4 section-pad">
            <%- include('./partials/banner-categorias', { categorias }) %>
        </section>
        <!-- RECOMENDADOS -->  
        <section class="section-pad">     
            <div class="separ-sect mt-5">
                <h4 class="section-title section-title-normal">                
                    <span>Recomendaciones de la casa...</span>
                    <b></b>
                </h4>
            </div>
            <%- include('./partials/recomendaciones', { destacados }) %>
        </section>
        <!-- ULTIMOS RESCATES --> 
        <section class="section-pad">
            <div class="separ-sect mt-5">
                <h4 class="section-title section-title-normal">                    
                    <span>Últimos rescates...</span>
                    <b></b>
                </h4>
                <%- include('./partials/ultimosRescates', { productoUltimosRescates }) %>

            </div>
        </section>
        <!-- INSTAGRAM -->
        <section class="section-pad">
            <div class="separ-sect mt-5">
                <h4 class="section-title section-title-center">
                    <b></b>
                    <span>Seguinos en Instagram</span>
                    <b></b>
                </h4>
                <div class=" mt-4 d-flex align-items-center">
                    <a href="https://www.instagram.com/gingeropavintage/" target="_blank" rel="noopener noreferrer" >
                        <img src="images/icons/Instagram_logo_2016.svg.webp" alt="" class="insta-icon ">
                    </a>
                        <a href="https://www.instagram.com/gingeropavintage/" target="_blank" rel="noopener noreferrer">
                            <h4>
                                @gingeropavintage
                            </h4>
                        </a>
                </div>
            </div>
        </section>
        <section>
            <%- !!instaData ? include('./partials/instagram-fetch', { instaData }) : include('./partials/instagram-fetch-error') %>
        </section>
        <div class="mt-4 mb-5 d-flex justify-content-center ">
            <a href="https://www.instagram.com/gingeropavintage/" target="_blank" rel="noopener noreferrer" >
                <button class="insta-button"> Seguinos en Instagram </button>
            </a>
        </div>

        <!-- Nosotrxs -->

        <section class="section-pad">
            <div class="separ-sect">                
                <hr class="hr-line">
            </div>
            <div class="d-flex align-items-center justify-content-center flex-column mt-4">
                <h3 class="bold">
                    Nosotrxs
                </h3>
                <h4 class="mt-3">
                    Bienvenidxs a Ginger Ropa Vintage
                </h4>
                <a href="" class="mt-3 mb-3">
                    <button class="insta-button"> Descubre más </button>
                </a>
            </div>
            <div class="separ-sect mb-5">                
                <hr class="hr-line">
            </div>
        </section>     
        
    </main>
    
    <footer>
        <%- include('./partials/footer') %>
    </footer>
    
    <!-- instagram js script -->
    <script src="js/insta.js"></script>
    <!-- BOOTSRAP ENDING LINE -->
    <script 
        src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" 
        integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" 
        crossorigin="anonymous">
    </script>

    <!-- incluimos cdn PARA OWL carousel -->
    
    <!-- O. JQUERY CDN -->
    <script 
        src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.4/jquery.min.js" 
        integrity="sha512-pumBsjNRGGqkPzKHndZMaAG+bir374sORyzM3uulLV14lN5LyykqNk8eEeUlUkB3U0M4FApyaHraT65ihJhDpQ==" 
        crossorigin="anonymous" 
        referrerpolicy="no-referrer">
    </script>
    <!-- 1. owl carouel min.js -->
    <script 
        src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js" 
        integrity="sha512-bPs7Ae6pVvhOSiIcyUClR7/q2OAsRiovw4vAkX+zJbw3ShAeeqezq50RIIcIURq7Oa20rW2n2q+fyXBNcU9lrw==" 
        crossorigin="anonymous" 
        referrerpolicy="no-referrer">
    </script>

    <!-- INICIAMOS OWL CAROSUEL -->
    <script src="js/owlCarousel.js"></script>   
    <!-- FOOTER -->
</body>
</html>